end)




ss:Button("Knock All",function()
for i,v in pairs(game:GetService("Players"):GetChildren()) do
if v ~= game.Players.LocalPlayer then
local args = {
    [1] = v.Character.Humanoid
}

game:GetService("ReplicatedStorage").RequiemKnock:FireServer(unpack(args))
end
end
end)

ss:Textbox("Freeze All For Added Time", true,function(t)

for i,v in pairs(game:GetService("Players"):GetChildren()) do
