    getgenv().auto = true
    local RunService = game:GetService("RunService")
    while RunService.RenderStepped:Wait() and getgenv().auto == true do
    
    for i,v in pairs(game:GetService("Players"):GetChildren()) do
    if v ~= game.Players.LocalPlayer then
    local args = {
        [1] = v.Backpack
    }
    
    game:GetService("ReplicatedStorage"):FindFirstChild("\201\147od \199\157\202\140\196\177\201\147pu\201\144\202\158\201\148\199\157\201\165\201\148~\240\157\147\133\240\157\147\133\198\164\225\151\157\225\142\182\240\159\144\165"):FireServer(unpack(args))
    
